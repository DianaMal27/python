def personal_info(**kwargs):
    return ' ' .join(kwargs.values())
print(personal_info(name = input("Введите ваше имя: "), surname = input("Введите вашу фамилию: "),
                    birthday = input("Введите дату рождения: "), city = input("Введите город проживания: "),
                    email = input("Введите ваш и-мэйл: "), phone_number = input("Введите номер телефона: ")))
