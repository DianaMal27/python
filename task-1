def myf(n_1, n_2):
    try:
        n_1, n_2 = int(n_1), int(n_2)
        myf_num = n_1 / n_2
    except ValueError:
        return "ValueError"
    except ZeroDivisionError:
        return "Деление на ноль запрещено"
    return round(myf_num, 4)
print(myf(input("Введите первое число - "), input("Введите второе число - ")))
